{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "maciej.mazur.site"
        }
      ],
      "destination": "/maciej/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "marcelina.mazur.site"
        }
      ],
      "destination": "/marcelina/:path*"
    }
  ]
}
