{
	"rewrites": [
		{
			"source": "/:path*",
			"has": [
				{
					"type": "host",
					"value": "maciej.mazur.site"
				}
			],
			"destination": "/maciej/:path*"
		},
		{
			"source": "/:path*",
			"has": [
				{
					"type": "host",
					"value": "marcelina.mazur.site"
				}
			],
			"destination": "/marcelina/:path*"
		},
		{
			"source": "/:path*",
			"has": [
				{
					"type": "host",
					"value": "mateusz.mazur.site"
				}
			],
			"destination": "https://mazur.site/mateusz/:path*"
		},
		{
			"source": "/:path*",
			"has": [
				{
					"type": "host",
					"value": "helena.mazur.site"
				}
			],
			"destination": "/helena/:path*"
		}
	]
}
